{
  "expo": {
    "name": "portal-cliente",
    "slug": "portal-cliente",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0171BB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#0171BB"
      }
    },
    "web": {
      "favicon": "./src/assets/icons/icon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}